--			ProdajaRabat1,
--			ProdajaRabat2,
--			ProdajaRabat3,
--			ProdajaVrednostDobavnice,
--			ProdajaŠtevilkaRačuna,
--			ProdajaDatumRačuna,
--			ProdajaPoslovniDogodekRačuna,
				ProdajaDobavitelj,
--			ProdajaOddelek,
--			ProdajaSkladišče,
--			ProdajaDobaviteljevaŠifra,
--			ProdajaEANKoda,
				ProdajaTipKupca,
--			ProdajaReferent,
--			ProdajaAcFieldSA,
--			ProdajaAcFieldSB,
--			ProdajaAcFieldSC,
--			ProdajaAcFieldSD,
--			ProdajaAcFieldSE,
--			ProdajaAcFieldSF,
--			ProdajaAcFieldSG,
--			ProdajaAcFieldSH,
--			ProdajaAcFieldSI,
--			ProdajaAcFieldSJ,
--			ProdajaAnFieldNA,
--			ProdajaAnFieldNB,
--			ProdajaAnFieldNC,
--			ProdajaAnFieldND,
--			ProdajaAnFieldNE,
--			ProdajaAnFieldNF,
--			ProdajaAnFieldNG,
--			ProdajaAnFieldNH,
--			ProdajaAnFieldNI,
--			ProdajaAnFieldNJ
				ISNULL(ProdajaZalogaSUM,0) as ProdajaZalogaSUM,
				ISNULL(ProdajaZalogaVrednost,0) as ProdajaZalogaVrednost
  FROM dbo.uTN_DB_ProdajaPoRealizaciji_DET ppr
	LEFT JOIN (SELECT ZalogaIdent, SUM(ZalogaKoličinaEM) AS ProdajaZalogaSUM, SUM(ZalogaVrednost) AS ProdajaZalogaVrednost FROM uTN_DB_Zaloge GROUP BY ZalogaIdent) za ON za.ZalogaIdent=ppr.ProdajaIdent;</Sql>
      </Query>
